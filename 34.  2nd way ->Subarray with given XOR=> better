public class Solution {
    public int solve(int[] A, int B) {
        int n=A.length;
        int cnt =0;
        for(int i=0 ;i<n;i++){
            int xorr =0
            for(int j=i;j<n;j++){
                xorr = xorr ^ a[j];
            
            if(xorr == B) cnt ++;
            }
        }
        return cnt;
    }
}
